{
  "name": "paycheck-to-paycheck",
  "version": "0.0.0",
  "scripts": {
    "build": "ng build",
    "clean": "rm -rf dist *.log",
    "deploy": "firebase deploy",
    "emulators": "firebase emulators:start",
